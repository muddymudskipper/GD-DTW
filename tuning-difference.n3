@prefix xsd:      <http://www.w3.org/2001/XMLSchema#> .
@prefix vamp:     <http://purl.org/ontology/vamp/> .
@prefix :         <#> .

:transform a vamp:Transform ;
    vamp:plugin <http://vamp-plugins.org/rdf/plugins/tuning-difference#tuning-difference> ;
    vamp:step_size "1024"^^xsd:int ; 
    vamp:block_size "1024"^^xsd:int ; 
    vamp:plugin_version """3""" ; 
    vamp:parameter_binding [
        vamp:parameter [ vamp:identifier "finetuning" ] ;
        vamp:value "1"^^xsd:float ;
    ] ;
    vamp:parameter_binding [
        vamp:parameter [ vamp:identifier "maxduration" ] ;
        vamp:value "0"^^xsd:float ;
    ] ;
    vamp:parameter_binding [
        vamp:parameter [ vamp:identifier "maxrange" ] ;
        vamp:value "3"^^xsd:float ;
    ] ;
    vamp:output <http://vamp-plugins.org/rdf/plugins/tuning-difference#tuning-difference_output_cents> .